<p>All methods can be called as <code>$().{{name}}('method', params);</code></p>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th style="width: 20%;">Method</th>
            <th style="width: 20%;">Parameters</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
      {{#each method}}
        <tr>
            <td>{{name}}{{new ../project.version}} {{since}}</td>
            <td>{{#with params}}{{>params}}{{/with}}</td>
            <td>{{md description}}
            {{#if example}}
                {{#source}}
                    {{{example}}}
                {{/source}}
            {{/if}}
            </td>
        </tr> 
       {{/each}}                             
    </tbody>
</table>